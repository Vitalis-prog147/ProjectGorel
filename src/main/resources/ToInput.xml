<?xml version="1.0" encoding="utf-8" ?>
<data>
    <!--Зона расстановки-->
    <placement_zone>
        <!-- Прямоугольник описывающий зону расстановки -->
        <rect>
            <min_point x="-50" y="-100"> </min_point>
            <max_point x="+50" y="+100"> </max_point>
        </rect>
        <!-- Прямоугольники описывающие зоны исключения(круги не должны попадать на них) -->
        <alarms>
            <alarm>
                <min_point x="-5" y="-100"> </min_point>
                <max_point x="+5" y="+100"> </max_point>
            </alarm>
            <alarm>
                <min_point x="-50" y="-100"> </min_point>
                <max_point x="-30" y="-80"> </max_point>
            </alarm>
        </alarms>
    </placement_zone>
    <!-- Круги которые необходимо разместить -->
    <circles>
        <circle id = "0" inner_rad = "12" outter_rad = "20"> </circle>
        <circle id = "1" inner_rad = "25" outter_rad = "30"> </circle>
        <circle id = "2" inner_rad = "32" outter_rad = "35"> </circle>
        <circle id = "3" inner_rad = "25" outter_rad = "30"> </circle>
        <circle id = "4" inner_rad = "12" outter_rad = "20"> </circle>
    </circles>
</data>
